    "@babel/plugin-syntax-class-properties": "^7.10.4",
    "@vladblindu/fetch-dog": "https://github.com/vladblindu/fetch-dog",
    "bootstrap": "^4.5.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.14.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
