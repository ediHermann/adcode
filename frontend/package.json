{
  "name": "@vladblindu/frontall",
  "version": "1.0.0",
  "description": "Frontend starter kit",
  "keywords": [
    "react",
    "tailwind",
    "frontend",
    "starter",
    "kit",
    "lib"
  ],
  "main": "dist/index.js",
  "types": "index.d.ts",
  "repository": "https://github.com/vladblindu/audiotext-comp-lib",
  "scripts": {
    "prepare": "rm -rf ./dist && touch .frontall/css/styles.css && yarn run build",
    "tw": "tailwindcss build .frontall/css/tailwind.css -o .frontall/css/styles.css",
    "start": "yarn run tw && webpack-dev-server  --open --config .frontall/webpack/webpack.development.js",
    "build": "webpack --config .frontall/webpack/webpack.production.js"
  },
  "author": "Vlad Blindu",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "clsx": "^1.1.1",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "formik": "^2.1.4",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "postcss": "^7.0.30",
    "prop-types": "^15.7.2",
    "react-datepicker": "^3.0.0",
    "react-inlinesvg": "^2.0.0",
    "react-router-dom": "^5.2.0",
